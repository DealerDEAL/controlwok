#include <iostream>

using namespace std;

int main()
{
    int sensor=-1;
    int enterenNumber;
    int  a;
    int result=0;
    std::cout << "" << std::endl;
    std::cin >> a;
    int *masive{new int[a]};
    for(int i=0;i>sensor;i++){
        cin>>enterenNumber
        if(enterenNumber!=sensor){
            
        }
        
        if (enterenNumber==sensor){
            for(int i=0;i<a;i++){
                result=masive[i]+result;
            }
            cout<<result;
            return 0;
        }
    }
}
